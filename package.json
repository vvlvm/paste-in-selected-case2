{
	"name": "paste-in-selected-case",
	"displayName": "Paste in Selected Case",
	"description": "",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.98.0"
	},
	"publisher": "vvlvm",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "github",
		"url": "https://github.com/vvlvm/paste-in-selected-case2"
	},
	"icon": "./icon.jfif",
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "paste-in-selected-case.pasteInSelectedCase",
				"title": "Paste in Selected Case"
			},
			{
				"command": "paste-in-selected-case.camelCase",
				"title": "Paste in Camel Case"
			},
			{
				"command": "paste-in-selected-case.titleCase",
				"title": "Paste in Title Case"
			},
			{
				"command": "paste-in-selected-case.constantCase",
				"title": "Paste in Constant Case"
			},
			{
				"command": "paste-in-selected-case.dotCase",
				"title": "Paste in Dot Case"
			},
			{
				"command": "paste-in-selected-case.paramCase",
				"title": "Paste in Param Case"
			},
			{
				"command": "paste-in-selected-case.upperCase",
				"title": "Paste in Upper Case"
			},
			{
				"command": "paste-in-selected-case.pathCase",
				"title": "Paste in Path Case"
			},
			{
				"command": "paste-in-selected-case.sentenceCase",
				"title": "Paste in Sentence Case"
			},
			{
				"command": "paste-in-selected-case.snakeCase",
				"title": "Paste in Snake Case"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "~20.19.33",
		"@types/vscode": "1.98.0",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.5.2",
		"esbuild": "^0.25.12",
		"eslint": "^9.39.2",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"change-case": "^5.4.4"
	}
}
